ALTER TABLE `busi_brand`
ADD COLUMN `create_time`  datetime NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
ADD COLUMN `create_by`  bigint NULL COMMENT '创建人id' ,
ADD COLUMN `update_time`  datetime NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '上次修改时间',
ADD COLUMN `update_by`  bigint NULL COMMENT '修改人id';